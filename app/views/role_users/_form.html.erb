<%= form_for @role_user do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :role_id %><br />
    <%= f.number_field :role_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
  </p>
  <p>
    <%= f.label :primary %><br />
    <%= f.check_box :primary %>
  </p>
  <p><%= f.submit %></p>
<% end %>
