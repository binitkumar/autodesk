<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", quotes_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_quote_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.show", :default => "Show")}", quote_path %></li>
    </ul>
  </div>
  <div class="content">
    <div class="inner">
      
        <p>
          <b><%= t("activerecord.attributes.quote.customer_id", :default => t("activerecord.labels.customer_id", :default => "Customer")) %>:</b>
          <%= @quote.customer_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.quote.dealer_id", :default => t("activerecord.labels.dealer_id", :default => "Dealer")) %>:</b>
          <%= @quote.dealer_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.quote.quote_type_id", :default => t("activerecord.labels.quote_type_id", :default => "Quote type")) %>:</b>
          <%= @quote.quote_type_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.quote.comment_originator_id", :default => t("activerecord.labels.comment_originator_id", :default => "Comment originator")) %>:</b>
          <%= @quote.comment_originator_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.quote.time", :default => t("activerecord.labels.time", :default => "Time")) %>:</b>
          <%= @quote.time %>
        </p>
      <div class="wat-cf">
        <%= link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Edit")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_quote_path(@quote), :class => "button" %>
        <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), quote_path(@quote), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
