<%= form_for @quote do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :customer_id %><br />
    <%= f.number_field :customer_id %>
  </p>
  <p>
    <%= f.label :dealer_id %><br />
    <%= f.number_field :dealer_id %>
  </p>
  <p>
    <%= f.label :quote_type_id %><br />
    <%= f.number_field :quote_type_id %>
  </p>
  <p>
    <%= f.label :comment_originator_id %><br />
    <%= f.number_field :comment_originator_id %>
  </p>
  <p>
    <%= f.label :time %><br />
    <%= f.time_select :time %>
  </p>
  <p><%= f.submit %></p>
<% end %>
