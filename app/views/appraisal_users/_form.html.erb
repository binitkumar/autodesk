<%= form_for @appraisal_user do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :appraisal_id %><br />
    <%= f.number_field :appraisal_id %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.number_field :user_id %>
  </p>
  <p>
    <%= f.label :role_id %><br />
    <%= f.number_field :role_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
