<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", registration_mark_vehicles_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_registration_mark_vehicle_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> RegistrationMarkVehicles</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.registration_mark_vehicle.registration_mark_id", :default => t("activerecord.labels.registration_mark_id", :default => "Registration_mark_id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @registration_mark_vehicles.each do |registration_mark_vehicle| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= registration_mark_vehicle.id %>
          </td>
                    <td>
            <%= link_to registration_mark_vehicle.registration_mark_id, registration_mark_vehicle_path(registration_mark_vehicle) %>
          </td>
                    <td>
            <%= registration_mark_vehicle.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", registration_mark_vehicle_path(registration_mark_vehicle) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_registration_mark_vehicle_path(registration_mark_vehicle) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", registration_mark_vehicle_path(registration_mark_vehicle), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
