<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", financial_transaction_sales_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_financial_transaction_sale_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.show", :default => "Show")}", financial_transaction_sale_path %></li>
    </ul>
  </div>
  <div class="content">
    <div class="inner">
      
        <p>
          <b><%= t("activerecord.attributes.financial_transaction_sale.financial_transaction_id", :default => t("activerecord.labels.financial_transaction_id", :default => "Financial transaction")) %>:</b>
          <%= @financial_transaction_sale.financial_transaction_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.financial_transaction_sale.sale_id", :default => t("activerecord.labels.sale_id", :default => "Sale")) %>:</b>
          <%= @financial_transaction_sale.sale_id %>
        </p>
      <div class="wat-cf">
        <%= link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Edit")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_financial_transaction_sale_path(@financial_transaction_sale), :class => "button" %>
        <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), financial_transaction_sale_path(@financial_transaction_sale), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
