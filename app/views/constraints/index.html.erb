<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", constraints_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_constraint_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> Constraints</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.constraint.constraint_type_id", :default => t("activerecord.labels.constraint_type_id", :default => "Constraint_type_id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @constraints.each do |constraint| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= constraint.id %>
          </td>
                    <td>
            <%= link_to constraint.constraint_type_id, constraint_path(constraint) %>
          </td>
                    <td>
            <%= constraint.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", constraint_path(constraint) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_constraint_path(constraint) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", constraint_path(constraint), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>


