<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", funding_plan_sales_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_funding_plan_sale_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> FundingPlanSales</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.funding_plan_sale.funding_plan_id", :default => t("activerecord.labels.funding_plan_id", :default => "Funding_plan_id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @funding_plan_sales.each do |funding_plan_sale| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= funding_plan_sale.id %>
          </td>
                    <td>
            <%= link_to funding_plan_sale.funding_plan_id, funding_plan_sale_path(funding_plan_sale) %>
          </td>
                    <td>
            <%= funding_plan_sale.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", funding_plan_sale_path(funding_plan_sale) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_funding_plan_sale_path(funding_plan_sale) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", funding_plan_sale_path(funding_plan_sale), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
