<%= form_for @reporting_category_metric do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :reporting_category_id %><br />
    <%= f.number_field :reporting_category_id %>
  </p>
  <p>
    <%= f.label :metric_id %><br />
    <%= f.number_field :metric_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>
