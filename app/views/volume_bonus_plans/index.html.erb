<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", volume_bonus_plans_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_volume_bonus_plan_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> VolumeBonusPlans</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.volume_bonus_plan.name", :default => t("activerecord.labels.name", :default => "Name")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @volume_bonus_plans.each do |volume_bonus_plan| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= volume_bonus_plan.id %>
          </td>
                    <td>
            <%= link_to volume_bonus_plan.name, volume_bonus_plan_path(volume_bonus_plan) %>
          </td>
                    <td>
            <%= volume_bonus_plan.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", volume_bonus_plan_path(volume_bonus_plan) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_volume_bonus_plan_path(volume_bonus_plan) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", volume_bonus_plan_path(volume_bonus_plan), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
