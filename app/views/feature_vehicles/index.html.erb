<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", feature_vehicles_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_feature_vehicle_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> FeatureVehicles</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.feature_vehicle.vehicle_id", :default => t("activerecord.labels.vehicle_id", :default => "Vehicle_id")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @feature_vehicles.each do |feature_vehicle| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= feature_vehicle.id %>
          </td>
                    <td>
            <%= link_to feature_vehicle.vehicle_id, feature_vehicle_path(feature_vehicle) %>
          </td>
                    <td>
            <%= feature_vehicle.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", feature_vehicle_path(feature_vehicle) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_feature_vehicle_path(feature_vehicle) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", feature_vehicle_path(feature_vehicle), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>


