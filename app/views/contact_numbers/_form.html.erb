<fieldset>
	<div class="control-group">
		<label class="control-label">Contact Number Type</label>
		<div class="controls">
			<%= contact_number.select(:contact_number_type_id, ContactNumberType.all, :id, :value, {}, {:class => "chzn-select"}) %>
		</div>
	</div>
	<div class="control-group">
		<label class="control-label">Value</label>
		<div class="controls">
			<%= contact_number.text_field :value, :class => 'input-large' %>
		</div>
	</div>
	<% if form_actions_visible == true %>
		<div class="form-actions">
			<%= render :partial => "shared/form_actions" %>
		</div>
	<% end %>
</fieldset>
