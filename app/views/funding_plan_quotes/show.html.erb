<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", funding_plan_quotes_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_funding_plan_quote_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.show", :default => "Show")}", funding_plan_quote_path %></li>
    </ul>
  </div>
  <div class="content">
    <div class="inner">
      
        <p>
          <b><%= t("activerecord.attributes.funding_plan_quote.funding_plan_id", :default => t("activerecord.labels.funding_plan_id", :default => "Funding plan")) %>:</b>
          <%= @funding_plan_quote.funding_plan_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.funding_plan_quote.quote_id", :default => t("activerecord.labels.quote_id", :default => "Quote")) %>:</b>
          <%= @funding_plan_quote.quote_id %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.funding_plan_quote.amount", :default => t("activerecord.labels.amount", :default => "Amount")) %>:</b>
          <%= @funding_plan_quote.amount %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.funding_plan_quote.fees_spread", :default => t("activerecord.labels.fees_spread", :default => "Fees spread")) %>:</b>
          <%= @funding_plan_quote.fees_spread %>
        </p>

        <p>
          <b><%= t("activerecord.attributes.funding_plan_quote.customer_rate", :default => t("activerecord.labels.customer_rate", :default => "Customer rate")) %>:</b>
          <%= @funding_plan_quote.customer_rate %>
        </p>
      <div class="wat-cf">
        <%= link_to image_tag("web-app-theme/icons/application_edit.png", :alt => "#{t("web-app-theme.edit", :default=> "Edit")}") + " " + t("web-app-theme.edit", :default=> "Edit"), edit_funding_plan_quote_path(@funding_plan_quote), :class => "button" %>
        <%= link_to image_tag("web-app-theme/icons/cross.png", :alt => "#{t("web-app-theme.delete", :default=> "Delete")}") + " " + t("web-app-theme.delete", :default => "Delete"), funding_plan_quote_path(@funding_plan_quote), :method => "delete", :class => "button", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
