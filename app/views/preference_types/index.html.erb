<% title "Preference Types" %>

<table>
  <tr>
    <th>Value</th>
  </tr>
  <% for preference_type in @preference_types %>
    <tr>
      <td><%= preference_type.value %></td>
      <td><%= link_to "Show", preference_type %></td>
      <td><%= link_to "Edit", edit_preference_type_path(preference_type) %></td>
      <td><%= link_to "Destroy", preference_type, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Preference Type", new_preference_type_path %></p>
