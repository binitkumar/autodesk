<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first active"><%= link_to "#{t("web-app-theme.list", :default => "List")}", preference_types_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_preference_type_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.all", :default => "All")  %> PreferenceTypes</h2>
    <div class="inner">
      <table class="table">
        <tr>
          <th class="first">ID</th>
                    <th>
            <%= t("activerecord.attributes.preference_type.value", :default => t("activerecord.labels.value", :default => "Value")) %>
          </th>
                    <th><%= t("web-app-theme.created_at", :default => "Created at")  %></th>
          <th class="last">&nbsp;</th>
        </tr>
        <% @preference_types.each do |preference_type| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <td>
            <%= preference_type.id %>
          </td>
                    <td>
            <%= link_to preference_type.value, preference_type_path(preference_type) %>
          </td>
                    <td>
            <%= preference_type.created_at %>
          </td>
          <td class="last">
            <%= link_to "#{t("web-app-theme.show", :default => "Show")}", preference_type_path(preference_type) %> |
            <%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_preference_type_path(preference_type) %> |
            <%= link_to "#{t("web-app-theme.delete", :default => "Delete")}", preference_type_path(preference_type), :method => :delete, :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
          </td>
        </tr>
        <% end -%>
      </table>
      <div class="actions-bar wat-cf">
        <div class="actions">
        </div>
        
      </div>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
