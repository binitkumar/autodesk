<%= form_for @address_postcode do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :address_id %><br />
    <%= f.number_field :address_id %>
  </p>
  <p>
    <%= f.label :postcode_id %><br />
    <%= f.number_field :postcode_id %>
  </p>
  <p>
    <%= f.label :order %><br />
    <%= f.number_field :order %>
  </p>
  <p><%= f.submit %></p>
<% end %>
