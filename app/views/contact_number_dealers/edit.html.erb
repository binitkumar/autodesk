<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", contact_number_dealers_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_contact_number_dealer_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_contact_number_dealer_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> ContactNumberDealer</h2>
    <div class="inner">
      <%= form_for @contact_number_dealer, :url => contact_number_dealer_path(@contact_number_dealer), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>

<% content_for :sidebar, render(:partial => 'sidebar') -%>
