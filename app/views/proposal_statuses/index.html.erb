<% title "Proposal Statuses" %>

<table>
  <tr>
    <th>Value</th>
  </tr>
  <% for proposal_status in @proposal_statuses %>
    <tr>
      <td><%= proposal_status.value %></td>
      <td><%= link_to "Show", proposal_status %></td>
      <td><%= link_to "Edit", edit_proposal_status_path(proposal_status) %></td>
      <td><%= link_to "Destroy", proposal_status, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Proposal Status", new_proposal_status_path %></p>
