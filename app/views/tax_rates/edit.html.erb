<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to "#{t("web-app-theme.list", :default => "List")}", tax_rates_path  %></li>
      <li><%= link_to "#{t("web-app-theme.new", :default => "New")}", new_tax_rate_path %></li>
      <li class="active"><%= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_tax_rate_path %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= t("web-app-theme.edit", :default => "Edit") %> TaxRate</h2>
    <div class="inner">
      <%= form_for @tax_rate, :url => tax_rate_path(@tax_rate), :html => { :class => :form } do |f| -%>
        <%= render :partial => "form", :locals => {:f => f} %>
      <% end -%>
    </div>
  </div>
</div>


